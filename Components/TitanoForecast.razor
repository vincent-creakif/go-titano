<Display If="Items is not null">
    <Display If="View == ForecastViewEnum.Yearly">
        <TitanoForecastYearly Items="Items" />
    </Display>
    <Display If="View == ForecastViewEnum.Weekly">
        <TitanoForecastWeekly Items="Items" />
    </Display>
</Display>

@code {
    [Parameter]
    public IReadOnlyCollection<TitanoForecastItem> Items { get; set; }
    [Parameter]
    public ForecastViewEnum View { get; set; }
}
